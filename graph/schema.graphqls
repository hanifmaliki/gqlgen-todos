type Query {
  getAnimeList: [Anime!]!
  getAnimeById(id: ID!): Anime!

  getPokemonList: [Pokemon!]!
  getPokemonById(id: ID!): Pokemon!

  getTodoList: [Todo!]!
}

type Mutation {
  createTodo(input: InputCreateTodo!): Todo!
  updateTodo(input: InputUpdateTodo!): Todo!
  deleteTodo(id: ID!): Todo!
}
